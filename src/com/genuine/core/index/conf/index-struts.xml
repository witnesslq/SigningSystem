<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="index" namespace="/index" extends="struts-default">
		<action name="index_*" class="com.genuine.core.index.action.IndexAction" method="{1}">
			<result name="{1}">/WEB-INF/jsp/front/index/{1}.jsp</result>
			<result name="topAdmin">/WEB-INF/jsp/admin/index/top.jsp</result>
			<result name="leftAdmin">/WEB-INF/jsp/admin/index/left.jsp</result>
		</action>
	</package>

</struts>
