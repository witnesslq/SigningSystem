<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="school" namespace="/school" extends="struts-default">
		<action name="school_*" class="com.genuine.front.login.action.SchoolAction" method="{1}">
			<result name="success">/WEB-INF/jsp/front/index/frame.jsp</result>
			<result name="error">/loginUI.jsp</result>
			<result name="logout" type="redirect">/loginUI.jsp</result>
		</action>
	</package>

</struts>
